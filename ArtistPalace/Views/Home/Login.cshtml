@model ArtistPalace.ViewModels.LoginViewModel

<form method="post">
    <h1>Login</h1>

    <div>
        <label for="email">Email:</label>
        <input id="email" type="text" asp-for="Email">
    </div>

    <div>
        <label for="pass">Password:</label>
        <input id="pass" type="password" asp-for="Password">
    </div>
    
    <button>Login</button>
</form>